topicprefix = /topic/mcollective
libdir = /usr/local/share/mcollective/plugins
logfile = /dev/null
loglevel = info

# Plugins 
securityprovider = psk
plugin.psk = <%= @config['psk'] %>

connector = stomp
plugin.stomp.host = <%= @config['queue_server'] %>
plugin.stomp.port = 6163
plugin.stomp.user = <%= @queue_user %>
plugin.stomp.password = <%= @queue_password %>

# Facts
factsource = opscodeohai
plugin.yaml = /etc/mcollective/facts.yaml

